@model QLHH.DAL.Model.EntityAttribute
@{
    var fieldName = ViewData["FieldName"];
}

<label class="col-md-2 control-label">@Model.Attribute.AttributeName</label>
<div class="col-md-10">
    <input type="text" class="form-control datepicker" name="@fieldName" data-date-format="dd/mm/yyyy" placeholder="chọn @Model.Attribute.AttributeName" value="@Model.EntityAttributeValue">
</div>
